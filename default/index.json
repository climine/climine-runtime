{
    "use": ["version", "v"],
    "require":{
        "cookies[count]":1
    },
    "define": {
        "count": "{{cookies[count].increment()}}",
        "product": "Runtime v{{ memory[version] }}",
        "list": "{{ product.split(' ') }}"
    },
    "cookie": [{"key": "count", "value":"{{count}}","expires": 3600, "path": "/", "domain": "localhost", "secure": true}]
}